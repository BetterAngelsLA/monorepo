{
  "presets": [
    [
      "@nx/react/babel",
      {
        "runtime": "automatic",
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-transform-export-namespace-from",
    "react-native-reanimated/plugin" // must be listed last
  ],
  "env": {
    "test": {
      "presets": ["babel-preset-expo"]
    }
  }
}

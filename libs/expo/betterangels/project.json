{
  "name": "expo-betterangels",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/expo/betterangels/src",
  "projectType": "library",
  "targets": {
    "generate-graphql-types": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/expo/betterangels",
        "command": "graphql-codegen --config codegen.ts"
      },
      "outputs": [
        "{workspaceRoot}/libs/expo/betterangels/src/lib/apollo/gql-types/"
      ]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/expo/betterangels/**/*.ts"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/expo/betterangels",
        "commands": [
          {
            "command": "tsc --noEmit -p tsconfig.lib.json"
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/expo/betterangels/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}

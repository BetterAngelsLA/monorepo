{
  "name": "react-icons",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/react/icons/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project react-icons --web",
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "libs/react/icons/.storybook",
        "port": 4400
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "configDir": "libs/react/icons/.storybook",
        "outputDir": "dist/storybook/react-icons"
      }
    }
  }
}

{
  "name": "storybook-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/storybook-web/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project storybook-web --web",
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "libs/storybook-web/.storybook",
        "port": 4400
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{projectRoot}/storybook-static"],
      "options": {
        "configDir": "libs/storybook-web/.storybook"
      }
    }
  }
}

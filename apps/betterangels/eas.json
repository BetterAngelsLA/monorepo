{
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1",
        "EXPO_PUBLIC_API_URL": "https://api.dev.betterangels.la",
        "EXPO_PUBLIC_APP_ENV": "development",
        "EXPO_PUBLIC_DEMO_API_URL": "http://localhost:8000",
        "EXPO_PUBLIC_NEW_RELIC_LOG_LEVEL": "INFO"
      },
      "channel": "development",
      "ios": {
        "simulator": true
      },
      "node": "22.14.0"
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1",
        "EXPO_PUBLIC_APP_ENV": "development",
        "EXPO_PUBLIC_API_URL": "https://api.dev.betterangels.la",
        "EXPO_PUBLIC_DEMO_API_URL": "http://localhost:8000",
        "EXPO_PUBLIC_NEW_RELIC_LOG_LEVEL": "INFO"
      },
      "node": "22.14.0"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_VARIANT": "production",
        "EAS_NO_FROZEN_LOCKFILE": "1",
        "EXPO_PUBLIC_APP_ENV": "production",
        "EXPO_PUBLIC_API_URL": "https://api.prod.betterangels.la",
        "EXPO_PUBLIC_DEMO_API_URL": "https://api.dev.betterangels.la",
        "EXPO_PUBLIC_NEW_RELIC_LOG_LEVEL": "WARN"
      },
      "channel": "production",
      "node": "22.14.0"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6468135540"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  },
  "cli": {
    "version": ">= 5"
  }
}
